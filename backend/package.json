{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --exclude=**/*.integration.test.ts --exclude=**/*.rateLimit.test.ts",
    "test:integration": "vitest run **/*.integration.test.ts --testTimeout=30000 --maxConcurrency=1",
    "test:rate-limit": "vitest run **/*.rateLimit.test.ts --testTimeout=30000 --maxConcurrency=1",
    "test:controllers": "vitest run src/controllers/__tests__/",
    "test:models": "vitest run src/models/__tests__/",
    "test:services": "vitest run src/services/__tests__/",
    "test:repositories": "vitest run src/repositories/__tests__/",
    "test:middleware": "vitest run src/middleware/__tests__/",
    "test:routes": "vitest run src/routes/__tests__/",
    "test:clean": "rm -rf coverage/ node_modules/.cache/",
    "test:debug": "DEBUG=* vitest run --reporter=verbose --testTimeout=30000 --bail=1",
    "test:verbose": "vitest run --reporter=verbose",
    "test:fail": "vitest run --reporter=default --reporter.summary=false",
    "test:ci": "vitest run --reporter=verbose --coverage=false --maxConcurrency=1",
    "test:quick": "vitest run --exclude=**/*.integration.test.ts --exclude=**/*.rateLimit.test.ts --exclude=**/*routes*.test.ts --testTimeout=10000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/express": "^4.17.23",
    "argon2": "^0.43.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "prisma": "^6.10.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^2.1.8"
  }
}
